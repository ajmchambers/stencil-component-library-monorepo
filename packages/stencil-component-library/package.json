{
  "name": "@ajmchambers/stencil-component-library",
  "version": "1.0.0-alpha.2",
  "description": "core stencil component library",
  "keywords": [
    "stenciljs",
    "webcomponents"
  ],
  "author": "Alan Chambers <ajmchambers@gmail.com>",
  "homepage": "https://github.com/ajmchambers/stencil-component-library-monorepo/tree/master/packages/stencil-component-library#readme",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-component-library/stencil-component-library.js",
  "directories": {
    "src": "./src"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^1.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajmchambers/stencil-component-library-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/ajmchambers/stencil-component-library-monorepo/issues"
  },
  "license": "MIT"
}
